import { combineLatest, of } from "rxjs";
import { switchMap, takeUntil } from "rxjs/operators";

const a = of("a");
const b = of("b");
const c = of("c");
const d = of("d");

const e = a.pipe(takeUntil(d), switchMap(_ => b)).subscribe();
