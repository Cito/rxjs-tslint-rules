import { of } from "rxjs";
import { publish, publishBehavior, publishLast, publishReplay, share, shareReplay, takeUntil } from "rxjs/operators";

const a = of("a");
const b = of("b");
const c = of("c");
const d = of("d");

const e = a.pipe(takeUntil(d), publish()).subscribe();
const f = a.pipe(takeUntil(d), publishBehavior("x")).subscribe();
const g = a.pipe(takeUntil(d), publishLast()).subscribe();
